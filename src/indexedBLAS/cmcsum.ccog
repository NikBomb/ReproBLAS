                    }
                  }else{
                    compression_0 = _mm256_set_ps(SMCOMPRESSION, 1.0, SMCOMPRESSION, 1.0, SMCOMPRESSION, 1.0, SMCOMPRESSION, 1.0);
                    expansion_0 = _mm256_set_ps(SMEXPANSION, 1.0, SMEXPANSION, 1.0, SMEXPANSION, 1.0, SMEXPANSION, 1.0);
                  }
                  for(i = 0; i + 4 <= N_block; i += 4, x += (incX * 8)){
                    x_0 = _mm256_set_ps(((float*)x)[((incX * 6) + 1)], ((float*)x)[(incX * 6)], ((float*)x)[((incX * 4) + 1)], ((float*)x)[(incX * 4)], ((float*)x)[((incX * 2) + 1)], ((float*)x)[(incX * 2)], ((float*)x)[1], ((float*)x)[0]);

                    q_0 = s_0_0;
                    s_0_0 = _mm256_add_ps(s_0_0, _mm256_or_ps(_mm256_mul_ps(x_0, compression_0), blp_mask_tmp));
                    q_0 = _mm256_sub_ps(q_0, s_0_0);
                    x_0 = _mm256_add_ps(x_0, _mm256_mul_ps(q_0, expansion_0));

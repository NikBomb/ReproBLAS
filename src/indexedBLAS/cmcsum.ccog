                  if(smindex0(manY)){
                    if(smindex0(manY + 1)){
                      compression_0 = _mm256_set1_ps(SMCOMPRESSION);
                      expansion_0 = _mm256_set1_ps(SMEXPANSION);
                    }else{
                      compression_0 = _mm256_set_ps(1.0, SMCOMPRESSION, 1.0, SMCOMPRESSION, 1.0, SMCOMPRESSION, 1.0, SMCOMPRESSION);

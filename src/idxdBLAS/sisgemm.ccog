cog.out(generate.generate(blockSize.BlockSize("sisgemm", "YT_BLOCK", 1, y_block, y_block, ["bench_rsgemm_AvgTransA_AvgTransB_fold_{}".format(terminal.get_sidefaultfold())]), cog.inFile, args, params, mode))
